#! /bin/sh

SED="/bin/sed"
prefix="/home/hisisdk/histb/autelan/release/usr"
datarootdir="/home/hisisdk/histb/autelan/release/usr/php"
exec_prefix="/home/hisisdk/histb/autelan/release/usr"
version="5.6.0"
vernum="50600"
include_dir="/home/hisisdk/histb/autelan/fakerelease/usr/php"
includes="-I$include_dir -I$include_dir/main -I$include_dir/TSRM -I$include_dir/Zend -I$include_dir/ext -I$include_dir/ext/date/lib"
ldflags=" -L/home/hisisdk/histb/autelan/release/usr/lib -L/home/hisisdk/histb/autelan/fakerelease/usr/lib"
libs="-lcrypt   -lz -lcrypt -lrt -lmcrypt -lz -lm  -lrt -lxml2 -lm -ldl -lxml2 -lm -ldl -lxml2 -lm -ldl -lcrypt -lxml2 -lm -ldl -lxml2 -lm -ldl -lxml2 -lm -ldl -lcrypt "
extension_dir='/home/hisisdk/histb/autelan/release/usr/lib/extensions/no-debug-non-zts-20131226'
man_dir=`eval echo /home/hisisdk/histb/autelan/fakerelease/usr/share/man`
program_prefix=""
program_suffix=""
exe_extension=""
php_cli_binary=NONE
php_cgi_binary=NONE
configure_options=" '--host=arm-hisiv200-linux' '--prefix=/home/hisisdk/histb/autelan/release/usr' '--bindir=/home/hisisdk/histb/autelan/release/usr/bin' '--sbindir=/home/hisisdk/histb/autelan/release/usr/sbin' '--libexecdir=/home/hisisdk/histb/autelan/release/usr/libexec' '--sysconfdir=/home/hisisdk/histb/autelan/release/etc' '--localstatedir=/home/hisisdk/histb/autelan/release/var' '--libdir=/home/hisisdk/histb/autelan/release/usr/lib' '--includedir=/home/hisisdk/histb/autelan/fakerelease/usr' '--datarootdir=/home/hisisdk/histb/autelan/fakerelease/usr/share' '--datadir=/home/hisisdk/histb/autelan/fakerelease/usr/share' '--infodir=/home/hisisdk/histb/autelan/fakerelease/usr/share/info' '--localedir=/home/hisisdk/histb/autelan/fakerelease/usr/share/local' '--mandir=/home/hisisdk/histb/autelan/fakerelease/usr/share/man' '--docdir=/home/hisisdk/histb/autelan/fakerelease/usr/share/doc' '--htmldir=/home/hisisdk/histb/autelan/fakerelease/usr/share/doc' '--dvidir=/home/hisisdk/histb/autelan/fakerelease/usr/share/doc' '--pdfdir=/home/hisisdk/histb/autelan/fakerelease/usr/share/doc' '--psdir=/home/hisisdk/histb/autelan/fakerelease/usr/share/doc' '--exec-prefix=/home/hisisdk/histb/autelan/release/usr' '--enable-fpm' '--enable-zip' '--disable-static' '--enable-shared' '--with-pic' '--without-pear' '--with-zlib-dir=/home/hisisdk/histb/autelan/release/usr' '--with-pcre-dir=/home/hisisdk/histb/autelan/release/usr' '--with-libxml-dir=/home/hisisdk/histb/autelan/release/usr' '--with-onig=/home/hisisdk/histb/autelan/release/usr' '--with-mcrypt=/home/hisisdk/histb/autelan/release/usr' 'host_alias=arm-hisiv200-linux' 'CC=arm-hisiv200-linux-gcc' 'CFLAGS= '-O2' '-I/opt/hisi-linux/x86-arm/arm-hisiv200-linux/target/usr/include' '-I/home/hisisdk/histb/autelan/fakerelease/usr/include' '-I/home/hisisdk/histb/autelan/fakerelease/usr/local/include' '-I/home/hisisdk/histb/autelan/fakerelease/usr/include/libxml2' 'LDFLAGS= '-L/opt/hisi-linux/x86-arm/arm-hisiv200-linux/target/lib' '-L/opt/hisi-linux/x86-arm/arm-hisiv200-linux/target/usr/lib' '-L/home/hisisdk/histb/autelan/release/lib' '-L/home/hisisdk/histb/autelan/release/usr/lib' '-L/home/hisisdk/histb/autelan/release/usr/local/lib' 'CXX=arm-hisiv200-linux-g++'"
php_sapis=" cli fpm cgi"

# Set php_cli_binary and php_cgi_binary if available
for sapi in $php_sapis; do
  case $sapi in
  cli)
    php_cli_binary="/home/hisisdk/histb/autelan/release/usr/bin/${program_prefix}php${program_suffix}${exe_extension}"
    ;;
  cgi)
    php_cgi_binary="/home/hisisdk/histb/autelan/release/usr/bin/${program_prefix}php-cgi${program_suffix}${exe_extension}"
    ;;
  esac
done

# Determine which (if any) php binary is available
if test "$php_cli_binary" != "NONE"; then
  php_binary="$php_cli_binary"
else
  php_binary="$php_cgi_binary"
fi

# Remove quotes
configure_options=`echo $configure_options | $SED -e "s#'##g"`

case "$1" in
--prefix)
  echo $prefix;;
--includes)
  echo $includes;;
--ldflags)
  echo $ldflags;;
--libs)
  echo $libs;;
--extension-dir)
  echo $extension_dir;;
--include-dir)
  echo $include_dir;;
--php-binary)
  echo $php_binary;;
--php-sapis)
  echo $php_sapis;;
--configure-options)
  echo $configure_options;;
--man-dir)
  echo $man_dir;;
--version)
  echo $version;;
--vernum)
  echo $vernum;;
*)
  cat << EOF
Usage: $0 [OPTION]
Options:
  --prefix            [$prefix]
  --includes          [$includes]
  --ldflags           [$ldflags]
  --libs              [$libs]
  --extension-dir     [$extension_dir]
  --include-dir       [$include_dir]
  --man-dir           [$man_dir]
  --php-binary        [$php_binary]
  --php-sapis         [$php_sapis]
  --configure-options [$configure_options]
  --version           [$version]
  --vernum            [$vernum]
EOF
  exit 1;;
esac

exit 0
